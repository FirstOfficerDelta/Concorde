<?xml version="1.0"?>
<PropertyList>
        <params>
  <emerg-switch>/controls/electric/ac/emergency/essential[2]</emerg-switch>
  <essential-switch>/controls/electric/ac/essential[2]</essential-switch>
 </params>
  <path>../Models/switch.ac</path>

<effect>
    <inherits-from>Aircraft/Concorde/Models/Effects/interior</inherits-from>
    <object-name>Nut</object-name>
    <object-name>Ball</object-name>
    <object-name>Switch</object-name>
</effect>

  <animation>
    <type>rotate</type>
    <object-name>Switch</object-name>
    <property>/controls/electric/ac/emergency/essential[2]</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>35</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>-35</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  
  
  <animation>
    <type>pick</type>
    <object-name>Switch</object-name>
    <action>
      <name>Captain INS Switch</name>
      <button>0</button>
      <repeatable>false</repeatable>
 
   <binding>
    <command>property-assign</command>
      <condition>
    <not>
     <property>/controls/electric/ac/emergency/essential[2]</property>
    </not>
   </condition>
    <property>/controls/electric/ac/emergency/generator/selected</property>
    <value type="bool">true</value>
   </binding>
      <binding>
        <command>property-toggle</command>
    <property>/controls/electric/ac/emergency/essential[2]</property>
    <step>1</step>
        <min>0</min>
        <max>1</max>
        <wrap>true</wrap>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>/controls/electric/ac/essential[2]</property>
        <step>1</step>
        <min>0</min>
        <max>1</max>
        <wrap>true</wrap>
      </binding>
     <binding>
        <command>property-toggle</command>
        <property>/instrumentation/generic/click</property>
      </binding>
    </action>
  </animation>


</PropertyList>

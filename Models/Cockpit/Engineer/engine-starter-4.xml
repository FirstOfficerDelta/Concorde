<?xml version="1.0"?>
<PropertyList>
  <params>
     <control-starter>/controls/engines/engine[3]/starter</control-starter>
  <control-relight>/systems/engines/engine[3]/relight</control-relight>
  <engine-relight>/systems/electrical/power/relight[3]</engine-relight>
  <air-pressure>/systems/air-bleed/power/cross[3]</air-pressure>
  </params>
  <path>../Models/switch.ac</path>

<effect>
    <inherits-from>Aircraft/Concorde/Models/Effects/interior</inherits-from>
    <object-name>Nut</object-name>
    <object-name>Ball</object-name>
    <object-name>Switch</object-name>
</effect>

  <animation>

    <type>rotate</type>
    <object-name>Switch</object-name>
    <property>/controls/engines/engine[3]/starter</property>
	
  
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>35</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>

  </animation>

  <animation>
    <type>pick</type>
    <object-name>Switch</object-name>
	<condition>
	<not>
    	<property>/systems/engines/engine[3]/relight</property>
	</not>
	<property>/systems/electrical/power/relight[3]</property>
	<property>/systems/air-bleed/power/cross[3]</property>
  	</condition>
    <action>
      <name>Captain INS Switch</name>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/engines/engine[3]/starter</property>
        <step>1</step>
        <min>0</min>
        <max>1</max>
        <wrap>true</wrap>
      </binding>
     <binding>
        <command>property-toggle</command>
        <property>/instrumentation/generic/click</property>
      </binding>
    </action>

</animation>





<animation>
    <type>rotate</type>
    <object-name>Switch</object-name>
    <property>/systems/engines/engine[3]/relight</property>
    <interpolation>
	<entry>
        <ind>1</ind>
        <dep>-35</dep>
      </entry>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>


  <animation>
    <type>pick</type>
    <object-name>Switch</object-name>
	<condition>
	<not>
	<property>/controls/engines/engine[3]/starter</property>
 	</not>	
	</condition>
    <action>
      <name>Relight</name>
	
      <button>1</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/systems/engines/engine[3]/relight</property>
        <step>1</step>
        <min>0</min>
        <max>1</max>
        <wrap>true</wrap>
      </binding>
     <binding>
        <command>property-toggle</command>
        <property>/instrumentation/generic/click</property>
      </binding>
    </action>
  </animation>


</PropertyList>

<?xml version="1.0"?>
<PropertyList>
  <params>
  <air-door>/controls/engines/schedule-auto</air-door>
  <door-auto>/controls/engines/schedule-low</door-auto>
 </params>

  <path>../Models/switch.ac</path>

<effect>
    <inherits-from>Aircraft/Concorde/Models/Effects/interior</inherits-from>
    <object-name>Nut</object-name>
    <object-name>Ball</object-name>
    <object-name>Switch</object-name>
</effect>

  <animation>

    <type>rotate</type>
    <object-name>Switch</object-name>
    <expression>
    
    <prod>
    <value>35</value>
    
    <prod>
    <sum>
    <value>-1</value>
    <prod>
    <property>/controls/engines/schedule-low</property>
	<value>2</value>
    </prod>
    </sum>
    
    <sum>
    <value>1</value>
    <prod>
    <property>/controls/engines/schedule-auto</property>
    <value>-1</value>
    </prod>
    </sum>
    </prod>
    
    </prod>
    
  </expression>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>

  </animation>

  <animation>
    <type>pick</type>
    <object-name>Switch</object-name>
	<condition>
	<not>
    <property>/controls/engines/schedule-auto</property>
	</not>
  	</condition>
    <action>
      <name>Captain INS Switch</name>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/engines/schedule-low</property>
        <step>1</step>
        <min>0</min>
        <max>1</max>
        <wrap>true</wrap>
      </binding>
     <binding>
        <command>property-toggle</command>
        <property>/instrumentation/generic/click</property>
      </binding>
    </action>

</animation>





  <animation>
    <type>pick</type>
    <object-name>Switch</object-name>
    
    <action>
      <name>Auto-trim</name>
      <button>1</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/engines/schedule-auto</property>
        <step>1</step>
        <min>0</min>
        <max>1</max>
        <wrap>true</wrap>
      </binding>
     <binding>
        <command>property-toggle</command>
        <property>/instrumentation/generic/click</property>
      </binding>
    </action>
  </animation>


</PropertyList>

<?xml version="1.0"?>
<PropertyList>
  
  <path>../Models/light_comparator.ac</path>

 
<effect>
    <inherits-from>Aircraft/Concorde/Models/Effects/interior</inherits-from>
    <object-name>Frame</object-name>
    <object-name>On</object-name>
    <object-name>Text</object-name>
</effect>




  
  
   <animation>
    <type>select</type>
    <object-name>On</object-name>
    <condition>
       <and>
     <greater-than>
    <property>/systems/electrical/outputs/bulb</property>
    <value>20.0</value>
   </greater-than>
      <or>
       
    <property>/controls/lighting/crew/engineer/center/test</property>
     <!-- bug -->
     <and>
     <not-equals>
         <property>/systems/temperature/group[2]/duct-degc</property>
         <property>/systems/temperature/group[3]/duct-degc</property>
        </not-equals>
        <property>/controls/temperature/group[2]/on</property>
        </and>
      </or>
      
      <not>
    <property>/controls/lighting/crew/engineer/center/low</property>
    </not>
     </and>
     
    </condition>

 
  </animation>
  
  
  <animation>
    <type>material</type>
    <object-name>Off</object-name>
     <condition>
       <and>
     <greater-than>
    <property>/systems/electrical/outputs/bulb</property>
    <value>20.0</value>
   </greater-than>
      <or>
       
    <property>/controls/lighting/crew/engineer/center/test</property>
     <!-- bug -->
       <and>
     <not-equals>
         <property>/systems/temperature/group[2]/duct-degc</property>
         <property>/systems/temperature/group[3]/duct-degc</property>
        </not-equals>
        <property>/controls/temperature/group[2]/on</property>
        </and>
      </or>
      
      
    <property>/controls/lighting/crew/engineer/center/low</property>
    
     </and>
     
    </condition>

    <emission>
      <red>0.5</red>
      <green>0.5</green>
      <blue>0.5</blue>
    </emission>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>Off</object-name>
    <condition>
    <not>
        <and>
     <greater-than>
    <property>/systems/electrical/outputs/bulb</property>
    <value>20.0</value>
   </greater-than>
      <or>
       
    <property>/controls/lighting/crew/engineer/center/test</property>
     <!-- bug -->
      <and>
     <not-equals>
         <property>/systems/temperature/group[2]/duct-degc</property>
         <property>/systems/temperature/group[3]/duct-degc</property>
        </not-equals>
        <property>/controls/temperature/group[2]/on</property>
        </and>
      </or>
      
      <not>
    <property>/controls/lighting/crew/engineer/center/low</property>
    </not>
     </and>
     
     </not>
    </condition>

    
  </animation>
  
  
  
  
 
  
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
  <emission>
     <red>0.2</red>
     <green>0.2</green>
     <blue>0.2</blue>
  </emission>
</animation>
</PropertyList>

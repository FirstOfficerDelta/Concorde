<?xml version="1.0"?>
<PropertyList>
  <path>../Models/acvolts.ac</path>
  

<effect>
    <inherits-from>Aircraft/Concorde/Models/Effects/interior</inherits-from>
    <object-name>Circle</object-name>
    <object-name>Guage</object-name>
    <object-name>Needle</object-name>

</effect>

<effect>
    <inherits-from>Aircraft/Concorde/Models/Effects/flash</inherits-from>
    <object-name>Circle</object-name>
    <object-name>Guage</object-name>
    <object-name>Needle</object-name>
    <object-name>Text</object-name>


</effect>

  

  <animation>
    

    <type>rotate</type>
    <object-name>Needle</object-name>
    
    <property>/instrumentation/voltmeter-ac/indicated-volt</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>120</dep>
      </entry>
      <entry>
        <ind>30</ind>
        <dep>45</dep>
      </entry>
      <entry>
        <ind>60</ind>
        <dep>15</dep>
      </entry>
      <entry>
        <ind>90</ind>
        <dep>-10</dep>
      </entry>
      <entry>
        <ind>120</ind>
        <dep>-65</dep>
      </entry>
      <entry>
        <ind>150</ind>
        <dep>-120</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  
  
  
  
    
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Guage</object-name>
    <object-name>Needle</object-name>

  <condition>
  <and>
    <property>controls/lighting/instrument-lights</property>
        <greater-than>
          <property>/systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </greater-than>
  </and>
  </condition>
  <emission>
   <red>0.7</red>
   <green>0.2</green>
   <blue>0.2</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
  </animation>
 

</PropertyList>

<?xml version="1.0" ?> 

<PropertyList> 
 <path>engineer-spotlight.ac</path> 

 <animation>
  <type>material</type>
  <object-name>Flood</object-name>
  <condition>
        <greater-than>
          <property>systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </greater-than>
  </condition>

  <emission>
   <red>0.83</red>
   <green>0.74</green>
   <blue>0.38</blue>
   <factor-prop>systems/lighting/level/engineer/panel</factor-prop>
  </emission>
 </animation> 

</PropertyList>

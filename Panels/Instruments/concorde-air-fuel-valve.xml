<?xml version="1.0"?>

<!-- Fuel valve selector -->

<PropertyList>

 <name>Fuel valve selector</name>
 <w-base>102</w-base>
 <h-base>153</h-base>

 <params>
  <valve-switch>/controls/pneumatic/engine[0]/fuel-valve</valve-switch>
  <valve-guard>/controls/pneumatic/engine[0]/fuel-valve-guard</valve-guard>
 </params>


 <layers>

  

  <layer>
   <name>Title off</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>OPEN</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-49</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>40</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title on</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>AUTO</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>rotation</type>
     <offset>90</offset>
    </transformation>
    <transformation>
     <type>x-shift</type>
     <offset>-10</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title 3</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>SHUT</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-47</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-55</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

 <actions>

 

 </actions>
 
</PropertyList>

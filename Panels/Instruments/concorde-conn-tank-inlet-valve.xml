<?xml version="1.0"?>

<!-- Tank 5 and 7 inlet valve. -->


<PropertyList>
 <name>Tank inlet valve</name>
 <w-base>50</w-base>
 <h-base>75</h-base>

 <params>
  <inlet-valve>/controls/fuel/tank[4]/inlet-valve</inlet-valve>
  <inlet-static>/controls/fuel/tank[4]/inlet-static</inlet-static>
 </params>


 <layers>
 

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>9</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>INLET</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-15</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>10</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title line 2</name>
   <type>text</type>
   <point-size>9</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>VALVE</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-17</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>1</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>

<?xml version="1.0"?>

<!-- RAT selector switch -->

<PropertyList>

 <name>RAT selector</name>
 <w-base>153</w-base>
 <h-base>102</h-base>

 <params>
  <rat-on>/systems/hydraulic/rat/selector[0]/on</rat-on>
  <rat-test>/systems/hydraulic/rat/selector[0]/test</rat-test>
  <rat-guard>/systems/hydraulic/rat/selector[0]/guard</rat-guard>
  <rat-num>/systems/hydraulic/rat/selector[0]/num</rat-num>
 </params>


 <layers>

 

<!-- labels -->

  <layer>
   <name>Title on</name>
   <type>text</type>
   <point-size>8</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>ON</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <condition>
       <equals>
        <property alias="../../../../../../../params/rat-num"/>
        <value>1</value>
       </equals>
     </condition>
     <offset>-30</offset>
    </transformation>
    <transformation>
     <type>x-shift</type>
     <condition>
       <equals>
        <property alias="../../../../../../../params/rat-num"/>
        <value>2</value>
       </equals>
     </condition>
     <offset>50</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>8</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title off</name>
   <type>text</type>
   <point-size>8</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>OFF</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <condition>
       <equals>
        <property alias="../../../../../../../params/rat-num"/>
        <value>1</value>
       </equals>
     </condition>
     <offset>-33</offset>
    </transformation>
    <transformation>
     <type>x-shift</type>
     <condition>
       <equals>
        <property alias="../../../../../../../params/rat-num"/>
        <value>2</value>
       </equals>
     </condition>
     <offset>47</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-17</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title 3</name>
   <type>text</type>
   <point-size>8</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>TEST</text>
    </chunk>
   </chunks>

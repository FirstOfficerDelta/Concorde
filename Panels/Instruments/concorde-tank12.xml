<!-- Fuel tank 12000 kg :

     - tanks 6 and 8.
     - tanks 9 to 11.
-->


<PropertyList>
 <name>Fuel Quantity</name>
 <w-base>160</w-base>
 <h-base>160</h-base>

 <params>
  <name>/consumables/fuel/tank[0]/name</name>
  <lbs-select>/consumables/fuel/tank[0]/level-gal_us</lbs-select>
 </params>

 <layers>

  

  <layer>
   <name>number</name>
   <type>switch</type>

   <layer>
    <name>Number 6, 8</name>
    <type>switch</type>
    <condition>
     <or>
      <equals>
       <property alias="../../../../../../../params/name"/>
       <value>6</value>
      </equals>
      <equals>
       <property alias="../../../../../../../params/name"/>
       <value>8</value>
      </equals>
     </or>
    </condition>

    <layer>
     <name>6</name>
     <condition>
      <equals>
       <property alias="../../../../../../../params/name"/>
       <value>6</value>
      </equals>
     </condition>

<?xml version="1.0"?>

<!-- Test of doors warning lights -->

<PropertyList>
 <name>Test Doors</name>
 <w-base>52</w-base>
 <h-base>78</h-base>


 <layers>

  

  <layer>
   <name>Title 1</name>
   <type>text</type>
   <point-size>9</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>DOORS</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-16</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>30</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title 2</name>
   <type>text</type>
   <point-size>9</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>TEST</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-11</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>18</offset>
    </transformation>
   </transformations>
  </layer>
 </layers>
 
 <actions>

  

 </actions>

</PropertyList>

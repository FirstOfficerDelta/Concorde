 <layers>

  

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>THRUST</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-55</offset>

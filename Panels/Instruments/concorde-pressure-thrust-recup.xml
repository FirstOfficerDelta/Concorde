    <transformation>
     <type>y-shift</type>
     <offset>45</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title 2</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>RECUPERATOR</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-65</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>30</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>

<?xml version="1.0"?>

<!-- Ditching valves :

     - double switch, because text would disturb the 2nd switch.
-->


<PropertyList>

 <name>Ditching valves</name>
 <w-base>182</w-base>
 <h-base>130</h-base>


 <layers>

<!-- SYSTEM 1 -->

  <layer>
   <name>Title system</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>SYS 1</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-60</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>40</offset>
    </transformation>
   </transformations>
  </layer>

  

<!-- SYSTEM 2 -->

  <layer>
   <name>Title system</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>SYS 2</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>50</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>50</offset>
    </transformation>
   </transformations>
  </layer>

  
<!-- TEXT -->

  <layer>
   <name>Title on</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>NORM</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>11</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title off</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>SHUT</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>3</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-43</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

 <actions>

  

 </actions>
 
</PropertyList>

<?xml version="1.0"?>

<PropertyList>

 <name>Directional Gyro / Magnetic switch</name>
 <w-base>51</w-base>
 <h-base>153</h-base>

 <params>
  <mode-sel>/instrumentation/compass[0]/mode-dg</mode-sel>
 </params>


 <layers>

<!-- translucent lens -->  
  

 

  <layer>
   <name>Title on</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>DG</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-11</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>35</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title off</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>MAG</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-10</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-42</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

 <actions>

 

 </actions>
 
</PropertyList>

<!-- Fuel tank 10000 kg :

     - tanks 5 and 7.
-->


<PropertyList>
 <name>Fuel Quantity</name>
 <w-base>160</w-base>
 <h-base>160</h-base>

 <params>
  <content-select>/consumables/fuel/tank[0]/level-gal_us</content-select>
 </params>

 <layers>

  

  <layer>
   <name>number</name>
   <type>switch</type>

   <layer>
    <name>5</name>
    <condition>
     <equals>
      <property alias="../../../../../../params/name"/>
      <value>5</value>
     </equals>
    </condition>
    <texture>
     <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-5.png</path>
     <x1>0</x1>
     <y1>0</y1>
     <x2>1</x2>
     <y2>1</y2>
    </texture>
    <w>25</w>
    <h>25</h>
   </layer>

   <layer>
    <name>7</name>
    <texture>
     <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-7.png</path>
     <x1>0</x1>
     <y1>0</y1>
     <x2>1</x2>
     <y2>1</y2>
    </texture>
    <w>25</w>
    <h>25</h>
   </layer>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-70</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-33</offset>
    </transformation>
   </transformations>
  </layer>
 

 </layers>
</PropertyList>

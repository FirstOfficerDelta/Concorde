<?xml version="1.0"?>

<!-- Group 3 or 4 control switch :
-->

<PropertyList>

 <name>Group switch</name>
 <w-base>153</w-base>
 <h-base>102</h-base>

 <params>
  <select-group>/controls/temperature/group[2]/on</select-group>
  <guard-group>/controls/temperature/group[2]/guard</guard-group>
 </params>


 <layers>

  
<!-- labels -->

  <layer>
   <name>Title on</name>
   <type>text</type>
   <point-size>8</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>ON</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-35</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>20</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title group</name>
   <type>text</type>
   <point-size>8</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>GROUP 3 OR 4</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-15</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-35</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title off</name>
   <type>text</type>
   <point-size>8</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>FAILED</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>45</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>20</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

 <actions>

 

 </actions>
 
</PropertyList>

<?xml version="1.0"?>

<!-- Interconnect valve, linking 2 tanks by gravity. -->


<PropertyList>
 <name>Interconnect valve</name>
 <w-base>125</w-base>
 <h-base>75</h-base>

 <params>
  <inter-valve>/controls/fuel/pumps/cross</inter-valve>
  <name>/consumables/fuel/tank[5]/name</name>
  <defined-status>/systems/electrical/power/specific</defined-status>
 </params>


 <layers>
  

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>9</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>INTER CON</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-27</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>18</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title valve</name>
   <type>switch</type>

   <layer>
    <name>Title 6 - 7</name>
    <condition>
     <equals>
      <property alias="../../../../../../params/name"/>
      <value>6</value>
     </equals>
    </condition>
    <type>text</type>
    <point-size>9</point-size>
    <color>
     <red>1.0</red>
     <green>1.0</green>
     <blue>1.0</blue>
    </color>
    <chunks>
     <chunk>
      <type>literal</type>
      <text>VALVE 6 - 7</text>
     </chunk>
    </chunks>
   </layer>

   <layer>
    <name>Title 5 - 8</name>
    <type>text</type>
    <point-size>9</point-size>
    <color>
     <red>1.0</red>
     <green>1.0</green>
     <blue>1.0</blue>
    </color>
    <chunks>
     <chunk>
      <type>literal</type>
      <text>VALVE 5 - 8</text>
     </chunk>
    </chunks>
   </layer>

   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-30</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-24</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>

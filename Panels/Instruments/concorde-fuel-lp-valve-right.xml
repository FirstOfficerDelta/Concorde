<?xml version="1.0"?>

<!-- LP valve selector :

     - shut with one of the 2 motors.
     - right labels.
-->

<PropertyList>

 <name>LP valve selector</name>
 <w-base>102</w-base>
 <h-base>102</h-base>

 <params>
  <select-valve>/controls/fuel/tank[1]/lp-valve</select-valve>
  <select-motor>/controls/fuel/tank[1]/lp-valve-motor</select-motor>
  <guard-valve>/controls/fuel/tank[1]/lp-valve-guard</guard-valve>
 </params>


 <layers>



<!-- labels -->

  <layer>
   <name>Title on</name>
   <type>text</type>
   <point-size>8</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>SHUT 1</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>15</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>35</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title 3</name>
   <type>text</type>
   <point-size>8</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>OFF</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>39</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>8</offset>
    </transformation>
    <transformation>
     <type>rotation</type>
     <offset>90</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title off</name>
   <type>text</type>
   <point-size>8</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>SHUT 2</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>15</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-48</offset>
    </transformation>
   </transformations>
  </layer>

<!-- guard -->

  
 </layers>

 <actions>

 

 </actions>
 
</PropertyList>

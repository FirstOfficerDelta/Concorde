<?xml version="1.0"?>

<!-- Concorde ADC test rotary selector.

-->

<PropertyList>
 <name>ADC rotary selector</name>
 <w-base>162</w-base>
 <h-base>108</h-base>

 <params>
  <selector>/controls/adc/system[0]/selector</selector>
 </params>


 <layers>

<!-- SELECTOR -->

  <layer>
   <name>Title 2</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>2</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-65</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-18</offset>
    </transformation>
   </transformations>
  </layer>


  <layer>
   <name>Title 1</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>1</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-60</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>4</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title monitoring</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>MON</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-49</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>21</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title normal</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>OFF</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-14</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>30</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title comparator</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>COMP</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>21</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>21</offset>
    </transformation>
   </transformations>
  </layer>

 

 </layers>

 <actions>

  

 </actions>

</PropertyList>

<?xml version="1.0"?>

<!-- Concorde INS mode selector.

-->

<PropertyList>
 <name>INS mode selector</name>
 <w-base>135</w-base>
 <h-base>108</h-base>

 <params>
  <mode-ctrl>/instrumentation/ins[0]/msu/mode</mode-ctrl>
 </params>


 <layers>

  <layer>
   <name>Title off</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>OFF</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-65</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>4</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title standby</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>STBY</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-55</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>16</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title align</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>ALIGN</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-16</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>25</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title nav</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>NAV</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>28</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>16</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title attitude</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>ATT</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>37</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>4</offset>
    </transformation>
   </transformations>
  </layer>

<!-- SELECTOR -->

 

 </layers>

 <actions>


 </actions>

</PropertyList>

<?xml version="1.0"?>

<!-- Concorde INS battery light :

     - automatic shutdown, by lack of battery.
-->

<PropertyList>

 <name>INS battery</name>
 <w-base>40</w-base>
 <h-base>80</h-base>

 <params>
  <mode-ctrl>/instrumentation/ins[0]/msu/mode</mode-ctrl>
  <test-ctrl>/instrumentation/ins[0]/control/test</test-ctrl>
  <service-select>/instrumentation/ins[0]/serviceable</service-select>
 </params>


 <layers>

  

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>9</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>BAT</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-10</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>25</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>

<?xml version="1.0"?>

<!-- Bus Tie Breaker switch -->

<PropertyList>

 <name>BTB selector</name>
 <w-base>102</w-base>
 <h-base>102</h-base>

 <params>
  <btb-switch>/controls/electric/ac/btb[0]</btb-switch>
  <btb-guard>/controls/electric/ac/btb-guard[0]</btb-guard>
 </params>


 <layers>

  <layer>
   <name>Title off</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>RESET</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-14</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>27</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title on</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>NORM</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-6</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-6</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title 3</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>TRIP</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-7</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-38</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

 <actions>


 </actions>
 
</PropertyList>
